\n\tcol=\r\nor:#2D572C;}\nspan.QuoteChar\n\t{mso-style-name:&quot;Quote Char&quot;;\n\tmso-style-prior=\r\nity:29;\n\tmso-style-link:Quote;\n\tfont-family:&quot;Palatino Linotype&quot;,&quot;serif&quot;;\n\tc=\r\nolor:#5A5A5A;\n\tfont-style:italic;}\nspan.IntenseQuoteChar\n\t{mso-style-name:&quot;=\r\nIntense Quote Char&quot;;\n\tmso-style-priority:30;\n\tmso-style-link:&quot;Intense Quote=\r\n&quot;;\n\tfont-family:&quot;Palatino Linotype&quot;,&quot;serif&quot;;\n\tcolor:#FFFDD0;\n\tbackground:#4=\r\nF81BD;\n\tfont-style:italic;}\nspan.EmailStyle46\n\t{mso-style-type:personal-com=\r\npose;\n\tfont-family:&quot;Palatino Linotype&quot;,&quot;serif&quot;;\n\tcolor:#2D572C;}\n.MsoChpDef=\r\nault\n\t{mso-style-type:export-only;}\n.MsoPapDefault\n\t{mso-style-type:export-=\r\nonly;\n\ttext-indent:.25in;}\n@page Section1\n\t{size:8.5in 11.0in;\n\tmargin:1.0i=\r\nn 1.0in 1.0in 1.0in;}\ndiv.Section1\n\t{page:Section1;}\n /* List Definitions *=\r\n/\n @list l0\n\t{mso-list-id:30690632;\n\tmso-list-type:hybrid;\n\tmso-list-templa=\r\nte-ids:-276787592 67698705 67698713 67698715 67698703 67698713 67698715 676=\r\n98703 67698713 67698715;}\n@list l0:level1\n\t{mso-level-text:&quot;%1&#92;)&quot;;\n\tmso-lev=\r\nel-tab-stop:none;\n\tmso-level-number-position:left;\n\ttext-indent:-.25in;}\n@l=\r\nist l1\n\t{mso-list-id:555318103;\n\tmso-list-type:hybrid;\n\tmso-list-template-i=\r\nds:-126305846 67698705 67698713 67698715 67698703 67698713 67698715 6769870=\r\n3 67698713 67698715;}\n@list l1:level1\n\t{mso-level-text:&quot;%1&#92;)&quot;;\n\tmso-level-t=\r\nab-stop:none;\n\tmso-level-number-position:left;\n\ttext-indent:-.25in;}\nol\n\t{m=\r\nargin-bottom:0in;}\nul\n\t{margin-bottom:0in;}\n--&gt;\n&lt;/style&gt;\n&lt;!--[if gte mso 9]=\r\n&gt;&lt;xml&gt;\n &lt;o:shapedefaults v:ext=3D&quot;edit&quot; spidmax=3D&quot;1026&quot; /&gt;\n&lt;/xml&gt;&lt;![endif]=\r\n--&gt;&lt;!--[if gte mso 9]&gt;&lt;xml&gt;\n &lt;o:shapelayout v:ext=3D&quot;edit&quot;&gt;\n  &lt;o:idmap v:ex=\r\nt=3